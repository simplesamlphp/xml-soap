{
    "name": "simplesamlphp/xml-soap",
    "description": "SimpleSAMLphp library for XML SOAP",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4 || ^8.0",
        "ext-dom": "*",
        "ext-soap": "*",

        "psr/log": "~1.1",
        "simplesamlphp/xml-common": "^0.8.8",
        "simplesamlphp/assert": "^0.2.13"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^1.1.7"
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\SOAP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\SOAP\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
