{
    "name": "simplesamlphp/xml-soap",
    "description": "SimpleSAMLphp library for XML SOAP",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-dom": "*",
        "ext-soap": "*",

        "psr/log": "^2.0 || ^3.0",
        "simplesamlphp/xml-common": "^1.0.1",
        "simplesamlphp/assert": "^1.0.0"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\SOAP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\SOAP\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
